{
  "name": "dam-edit-framework-www",
  "version": "1.0.0",
  "description": "Base framework for building web applications upon the DAM",
  "engines": {
    "node": "5.4.0"
  },
  "scripts": {
    "dev": "npm run lint && npm run css:build && LOG_LEVEL=DEBUG nodemon index.js | bunyan -o short & npm run test:watch",
    "start": "npm run build && node index.js",
    "build:webpack": "./node_modules/.bin/webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack && npm run css:build && npm run copy:dist",
    "clean": "./node_modules/.bin/rimraf dist",
    "copy:dist": "cp -R public/assets dist/assets && cp public/favicon.ico dist/favicon.ico",
    "css": "npm run css:build && browser-sync start --port 3334 --proxy 'http://localhost:8080' --files 'public/assets/styles/styles.css' & npm run css:watch",
    "css:build": "./node_modules/.bin/node-sass src/client/styles/index.scss public/assets/styles/styles.css --precision 10",
    "css:watch": "npm run css:build && ./node_modules/.bin/node-sass -w src/client/styles/index.scss public/assets/styles/styles.css --precision 10",
    "debug-server": "nodemon debug-server.js",
    "inspect": "./node_modules/.bin/node-inspector & nodemon --debug index.js",
    "lint": "./node_modules/.bin/eslint src",
    "test": "node karma.server.js",
    "test:watch": "node karma.server.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TimeInc/dam-edit-framework-www.git"
  },
  "keywords": [],
  "authors": [
    "Christopher Pappas <christopher.pappas@postlight.com>",
    "Jim Nelson <jim.nelson@postlight.com>",
    "Jillian Kramer <jill.kramer@timeinc.com>",
    "Mark Leich <mark_leich@timeinc.com>"
  ],
  "license": "Unlicense",
  "homepage": "https://github.com/TimeInc/dam-edit-framework-www",
